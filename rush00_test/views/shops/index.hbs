{{# each products}}
<!--<h1>{{title}}</h1>
<p>Welcome to {{title}}</p> -->
<div class="row">
  {{# each this}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{ this.imagePath }}" alt="kpop" class="img-responsive">
      <div class="caption">
        <h3>{{ this.title }}</h3>
        <p class="description">{{ this.description }}</p>
        <div class="clearfix">
          <!--<a href="#" class="btn btn-primary" role="button">Button</a>-->
          <div class="price pull-left">${{ this.price }}</div>
          <a href="#" class="btn btn-success pull-right" role="button">Add</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}